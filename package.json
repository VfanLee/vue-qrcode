{
  "name": "vue-qrcode",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "module": "dist/vue-qrcode.js",
  "main": "dist/vue-qrcode.umd.js",
  "types": "dist/index.d.ts",
  "files": [
